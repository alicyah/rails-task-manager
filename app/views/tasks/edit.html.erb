<div class="container">
  <h1>Edit task</h1>
  <%= form_for(@task) do |f| %>
    <div class="form-group">
      <%= f.check_box :completed %>
      <%= f.label :completed %>
    </div>
  <% end %>

  <%= render 'form', task: @task %>

  <div class="inner-links"><%= link_to "Back to list", tasks_path %></div>
</div>
